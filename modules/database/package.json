{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@erento/nestjs-module-database",
  "version": "1.0.0",
  "description": "NestJS module to work with DB with sequelize.",
  "main": "dist/public_api.js",
  "types": "./dist/public_api.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prebuild": "rm -rf dist && npm t",
    "publish": "echo \"This module is not prepared to be published, yet.\n\" && false && exit 1",
    "test": "jest"
  },
  "keywords": [],
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/erento/nestjs-modules.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": ">=5.3.9",
    "typescript": ">=2.7"
  },
  "dependencies": {
    "aws-sdk": "2.320.0",
    "sequelize": "4.38.1",
    "sequelize-typescript": "0.6.6"
  },
  "devDependencies": {
    "@nestjs/common": "5.3.9",
    "@types/jest": "23.3.2",
    "@types/node": "10.10.3",
    "jest": "23.6.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.2",
    "ts-jest": "23.10.1",
    "typescript": "2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
